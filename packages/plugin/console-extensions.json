[
  {
    "type": "console.page/route",
    "properties": {
      "exact": true,
      "path": "/example",
      "component": { "$codeRef": "ExamplePage" }
    }
  },
  {
    "type": "console.navigation/href",
    "properties": {
      "id": "example",
      "name": "%plugin__hawtio-online-console-plugin~Plugin Example%",
      "href": "/example",
      "perspective": "admin",
      "section": "example-section"
    }
  },
  {
    "type": "console.navigation/section",
    "properties": {
      "id": "example-section",
      "insertAfter": "builds",
      "perspective": "admin",
      "name": "Examples"
    }
  },
  {
    "type": "console.tab/horizontalNav",
    "properties": {
      "page": {
        "name": "HawtIO",
        "href": "hawtio"
      },
      "model": {
        "group": "core",
        "version": "v1",
        "kind": "Pod"
      },
      "component": { "$codeRef": "HawtioMainTab" }
    }
  },
  {
    "type": "console.user-preference/group",
    "properties": {
      "id": "hawtioPreferences",
      "label": "HawtIO"
    }
  },
  {
    "type": "console.user-preference/item",
    "properties": {
      "id": "hawtio-preferences-content",
      "groupId": "hawtio-preferences",
      "label": "HawtIO",
      "field": {
        "type": "custom",
        "component": { "$codeRef": "HawtioMainPrefs" }
      }
    }
  }
]
