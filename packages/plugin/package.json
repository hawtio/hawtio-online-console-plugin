{
  "name": "@hawtio/online-console-plugin",
  "version": "0.2.0-alpha1",
  "description": "Plugin for deployment in OpenShift Console",
  "author": "Hawtio developer team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/hawtio/hawtio-online-console-plugin.git"
  },
  "scripts": {
    "start:plugin": "yarn clean && NODE_ENV=development yarn webpack serve --progress --profile --json=compilation-stats.json --env PLUGIN_NAME=$npm_package_name --env PACKAGE_VERSION=$npm_package_version --config webpack.config.dev.js",
    "start:console": "yarn bridge-auth && yarn cert-gen && ./scripts/start-console.sh",
    "cert-gen": "cd tls-certificates && ./generate-dev-certs.sh",
    "bridge-auth": "cd bridge-auth-https && ./setup-oauth.sh",
    "build": "yarn clean && NODE_ENV=production webpack --env PLUGIN_NAME=$npm_package_name --env PACKAGE_VERSION=$npm_package_version --config webpack.config.prod.js",
    "clean": "rm -rf dist",
    "webpack": "node -r ts-node/register --inspect ../../node_modules/.bin/webpack"
  },
  "devDependencies": {
    "@hawtio/react": "1.9.1-react17.memoryrouter.dev.2",
    "@openshift-console/dynamic-plugin-sdk": "1.8.0",
    "@openshift-console/dynamic-plugin-sdk-webpack": "1.3.0",
    "@patternfly/patternfly": "^5.3.1",
    "@patternfly/react-core": "^5.3.3",
    "@patternfly/react-icons": "^5.3.2",
    "@patternfly/react-styles": "^5.3.1",
    "@types/jsonpath": "^0.2.4",
    "@types/node": "^20.14.9",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.26",
    "@types/react-router-dom": "^5.3.2",
    "compression-webpack-plugin": "^11.1.0",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "cz-conventional-changelog": "3.3.0",
    "dotenv-webpack": "^8.1.0",
    "fetch-intercept": "^2.4.0",
    "html-webpack-plugin": "^5.6.0",
    "interpolate-html-plugin": "^4.0.0",
    "jsonpath": "^1.1.1",
    "kubernetes-types": "^1.30.0",
    "mocha-junit-reporter": "^2.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "pluralize": "^8.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "5.3.x",
    "react-router-dom": "5.3.x",
    "sass": "^1.84.0",
    "sass-loader": "^16.0.4",
    "source-map-loader": "^5.0.0",
    "stream-http": "^3.2.0",
    "style-loader": "^4.0.0",
    "stylelint": "^15.3.0",
    "stylelint-config-standard": "^31.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.5",
    "url": "^0.11.4",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-merge": "^5.9.0",
    "whatwg-fetch": "^3.6.20"
  },
  "peerDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "babel-loader": "^9.2.1",
    "mocha": "^10.5.1"
  },
  "resolutions": {
    "@types/react": "^17.0.37",
    "@monaco-editor/react": "4.7.0"
  }
}
